{
  "name": "crontab-react",
  "private": false,
  "version": "1.2.0",
  "description": "Crontab React Component",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preversion": "npm run build",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "postversion": "git push origin && git push origin --tags && npm publish --access public"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-mdx-gfm": "^7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^5.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.4",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/crontab-react.umd.js",
  "module": "./dist/crontab-react.es.js",
  "exports": {
    ".": {
      "import": "./dist/crontab-react.es.js",
      "require": "./dist/crontab-react.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhavalwd/react-crontab.git"
  },
  "keywords": [
    "react",
    "crontab",
    "schedule",
    "storybook",
    "django-celery-beat"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhavalwd/react-crontab/issues"
  },
  "homepage": "https://dhavalwd.github.io/crontab-react",
  "dependencies": {
    "react-select": "^5.7.3"
  }
}
